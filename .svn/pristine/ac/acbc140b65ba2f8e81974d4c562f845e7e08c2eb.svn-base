package com.jxc.common.util;

public class ImgBase64RegexUtil {

    private static final String regexForUpload = "suffix:(jpg|jpeg|png),[A-Za-z\\d+/]{0,}";

    public static boolean regexBase64ImgStrForUpload(String base64ImgStr){
        return base64ImgStr.matches(regexForUpload);
    }

    public static void main(String[] args) {
        String imgStr = "suffix:jpg,";

//        String imgStr = "suffix:jpg,";
        System.out.println(regexBase64ImgStrForUpload(imgStr));

    }

}
