package com.jxc.user_center.entity;

import java.io.Serializable;

import com.baomidou.mybatisplus.annotations.TableName;

@TableName("jxc_child_account")
public class ChildAccount implements Serializable{
	private static final long serialVersionUID = 1L;
	
	private Long childId;
	
	private Long ownerId;
	
	private Long driverId;
	
	private Integer status;
	
	private Integer workState;
	
	private Long machineId;
	
	private String childImg;
	
	private String phone;
	/**
	 * 车牌号
	 */
	private String machineCardNo;

	public Long getChildId() {
		return childId;
	}

	public void setChildId(Long childId) {
		this.childId = childId;
	}

	public Long getOwnerId() {
		return ownerId;
	}

	public void setOwnerId(Long ownerId) {
		this.ownerId = ownerId;
	}

	public Long getDriverId() {
		return driverId;
	}

	public void setDriverId(Long driverId) {
		this.driverId = driverId;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getWorkState() {
		return workState;
	}

	public void setWorkState(Integer workState) {
		this.workState = workState;
	}

	public Long getMachineId() {
		return machineId;
	}

	public void setMachineId(Long machineId) {
		this.machineId = machineId;
	}

	public String getMachineCardNo() {
		return machineCardNo;
	}

	public void setMachineCardNo(String machineCardNo) {
		this.machineCardNo = machineCardNo;
	}

	public String getChildImg() {
		return childImg;
	}

	public void setChildImg(String childImg) {
		this.childImg = childImg;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}
	
	

}
